{
  "name": "@dontepsu/aws-lambda-router",
  "version": "1.0.0-beta.0",
  "description": "Simple API router for AWS lambda and Serverless framework built with TypeScript",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepare": "npm run build",
    "clean": "rm -rf ./lib",
    "test": "jest --detectOpenHandles",
    "test:tdd": "jest --detectOpenHandles --watch",
    "lint": "tslint --project .",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dontepsu/aws-lambda-router.git"
  },
  "author": "Patrik Keinonen <patrik@wirmark.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dontepsu/aws-lambda-router/issues"
  },
  "homepage": "https://github.com/dontepsu/aws-lambda-router#readme",
  "dependencies": {
    "@hapi/boom": "^8.0.1",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.27",
    "@types/aws-sdk": "^2.7.0",
    "@types/node": "^12.7.12",
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.3",
    "tslint": "^5.18.0",
    "tslint-config-semistandard": "^8.0.1"
  },
  "jest": {
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\/src/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ]
  }
}
